{
  "name": "discord-bot-template-ts",
  "version": "1.0.0",
  "description": "Template Discord Bot TypeScript",
  "main": "./build/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --poll --transpile-only --ignore-watch ./node_modules ./src/app.ts",
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "start": "node ."
  },
  "keywords": [
    "discord",
    "bot",
    "template",
    "typescript",
    "v14"
  ],
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.0",
    "ms": "^2.1.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.23",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
